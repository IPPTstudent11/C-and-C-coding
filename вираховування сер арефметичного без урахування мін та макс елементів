#include <stdio.h>

int main() {
    int array[][7] = {
        {10, 100, 852, 5},
        {878, 5145, 74},
        {8, 8, 5, 40, 22, 23, 21}
    };

    int total_elements = 0;
    int count = 0;
    int i;

    for (i = 0; i < sizeof(array) / sizeof(array[0]); i++) {
        total_elements += sizeof(array[i]) / sizeof(int);
    }

    int result[total_elements];
    int j;

    for (i = 0; i < sizeof(array) / sizeof(array[0]); i++) {
        for (j = 0; j < sizeof(array[i]) / sizeof(int); j++) {
            result[count++] = array[i][j];
        }
    }

    int sum = 0;
    for (i = 0; i < total_elements; i++) {
        sum += result[i];
    }

    int min = result[0];
    int max = result[0];

    for (i = 1; i < total_elements; i++) {
        if (result[i] < min) {
            min = result[i];
        }
        if (result[i] > max) {
            max = result[i];
        }
    }

    int average = (sum - min - max) / (total_elements - 2);

    printf("serednye arefmetuchne ne vrahovuyuchu min ta max elementiv: %d\n", average);

    return 0;
